.container-fluid {
  margin-bottom: 100px;
  padding-top: 5px;
}

.table > tbody > tr:first-child > td, .table > tbody > tr:first-child > th {
    border: none;
}

.content-container {
  margin-bottom: 20px;
}

.small-logo {
  margin-right: 0px;
  margin-left: 0px;
}

.likes-count {
  font-size: 18px;
}

.help {
/*  margin-top: 20px; */
}

.form-input {
  border-radius: 0;
  height: 34px;
  padding: 6px 12px;
  font-size: 14px;
  line-height: 1.428571429;
  color: #555555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}

.form-space {
  margin-top: 20px;
}

.wide-form-input {
  width: 100%;
}

.icon-bar {
  margin-top: 20px;
}

.global-search-form {
  margin-top: 8px;
  margin-bottom: 20px;
}

.navbar-fixed-bottom {
  text-align: center;
  padding: 10px;
  min-height: 10px !important;
  background-color: #fff !important;
}

.green-flower-separator {
  text-align: center;
  padding-top: 30px;
  padding-bottom: 10px;
}

.likes-count-small {
  text-align: center;
  font-weight: bold;
}
