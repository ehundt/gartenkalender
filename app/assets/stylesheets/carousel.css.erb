.carousel-caption h2 {
  color: #333;
  text-shadow:  0px 0px 0px rgba(0, 0, 0, 0);
}

.carousel-caption h4 {
  color: #333;
  text-shadow:  0px 0px 0px rgba(0, 0, 0, 0);
}

.carousel-caption h4:first-child {
  animation-delay: 0.5s;
}

.carousel-caption h4:nth-child(2) {
  animation-delay: 1.5s;
}

.carousel-caption h4:nth-child(3) {
  animation-delay: 2.5s;
}

.carousel-caption h4:nth-child(4) {
  animation-delay: 3.5s;
}

.carousel-caption h4:nth-child(5) {
  animation-delay: 4.5s;
}

.carousel-caption h4:nth-child(6) {
  animation-delay: 5.5s;
}

.carousel-caption img {
  animation-delay: 4.5s;
}

.carousel-caption video {
  animation-delay: 2s;
  text-align: center;
}

.carousel-caption div {
  animation-delay: 3.5s;
}

.carousel-caption button {
  animation-delay: 1.5s;
}

.carousel .item {
  min-height: 650px;
  background-color: #fff;
}

.carousel-control.left {
  background-image: none !important;
}

.carousel-control.right {
  background-image: none !important;
}

.carousel-control .glyphicon-chevron-left, .carousel-control .glyphicon-chevron-right {
  color: #095;
}

.carousel-indicators .active {
  background-color: #095 !important;
}

.carousel-indicators li {
  border: 1px solid #095 !important;
}

.slide-0 {
  margin-top: 50px;
}

.slide-1 {
  margin-top: 50px;
}

.slide-2 {
  margin-top: 30px;
}

.slide-3 {
  margin-top: 30px;
}

.slide-4 {
  margin-top: 30px;
}

.slide-5 {
  margin-top: 40px;
}

.slide-5-bottom {
  margin-top: 100px;
}

.slide-6 {
  margin-top: 40px;
}

.slide-7 {
  margin-top: 40px;
}

#screencast-container {
  height: 460px;
  width: 961px;
  margin-left: -10px;
  background-color: #095;
  vertical-align: center;
  padding: 5px;
  margin-top: 20px;
  margin-bottom: 20px;
}
