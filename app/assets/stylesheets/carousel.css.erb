.carousel-caption h2 {
  color: #333;
  text-shadow:  0px 0px 0px rgba(0, 0, 0, 0);
}

.carousel-caption h4 {
  color: #333;
  text-shadow:  0px 0px 0px rgba(0, 0, 0, 0);
}

.carousel-no-image {
  padding: 10px;
}

.carousel-caption h2:nth-child(2) {
  animation-delay: 0.5s;
}

.carousel-caption h4:first-child {
  animation-delay: 0s;
}

.carousel-caption h4:nth-child(2) {
  animation-delay: 1.0s;
}

.carousel-caption h4:nth-child(3) {
  animation-delay: 2.5s;
}

.carousel-caption h4:nth-child(4) {
  animation-delay: 4.0s;
}

.carousel-caption h4:nth-child(5) {
  animation-delay: 5.5s;
}

.carousel-caption h4:nth-child(6) {
  animation-delay: 7.0s;
}

.carousel .item {
  min-height: 470px;
  background-color: #fff;
}

.carousel-contro.left {
  background-image: none !important;
}

.carousel-contro.right {
  background-image: none !important;
}

.carousel-control .glyphicon-chevron-left, .carousel-control .glyphicon-chevron-right {
  color: #095;
}

.carousel-indicators .active {
  background-color: #095 !important;
}

.carousel-indicators li {
  border: 1px solid #095 !important;
}

.carousel-image {
  background-color: #095;
  vertical-align: center;
  padding: 5px;
  margin-top: 20px;
}

#screencast-container {
  height: 460px;
  width: 961px;
  margin-left: -10px;
  background-color: #095;
  vertical-align: center;
  padding: 5px;
  margin-top: 20px;
  margin-bottom: 20px;
}

.carousel-caption video {
  animation-delay: 2s;
  text-align: center;
}

