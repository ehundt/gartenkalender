.head-bg {
  background: center repeat-x url('<%= image_url("rasen.png") %>');
  height: 120px;
  margin-bottom: 10px;
}

.head {
  margin-top: 38px;
}

.logo, .logo:hover {
  padding-top: 50px;
  text-align: center;
  text-decoration: none;
  color: #636c2d;
}

.alert-notice {
  background-color: #dff0d8;
  border-color: #d6e9c6;
  color: #3c763d;
}

.alert-notice hr {
  border-top-color: #c9e2b3;
}

.alert-notice .alert-link {
  color: #2b542c;
}

.alert {
  color: #2b542c;
  background-color: #dff0d8;
}
