.head {
  background: center repeat-x url('<%= image_url("clematis_120px.png") %>');
  height: 120px;
  margin-bottom: 30px;
}

.logo, .logo:hover {
  padding-top: 10px;
  text-align: center;
  text-decoration: none;
  color: #51561f;
}

.alert-notice {
  background-color: #dff0d8;
  border-color: #d6e9c6;
  color: #3c763d;
}

.alert-notice hr {
  border-top-color: #c9e2b3;
}

.alert-notice .alert-link {
  color: #2b542c;
}
